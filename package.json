{
  "name": "sketch-design",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --proxy-config=proxy/dev-proxy.conf.json --port=4203 --c=dev -o",
    "start:prod": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --proxy-config=proxy/dev-proxy.conf.json --port=4203 --c=release-site -o",
    "build:prod": "npm run build:prod:release-site",
    "build:prod:release-site": "node ./node_modules/@angular/cli/bin/ng build --c=release-site",
    "build:prod:release-site:more-mem": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng build --c=release-site",
    "build:prod:dev-site": "node ./node_modules/@angular/cli/bin/ng build --c=dev-site",
    "build:prod:dev-site:more-mem": "node --max_old_space_size=6000 ./node_modules/@angular/cli/bin/ng build --c=dev-site",
    "format:check": "prettier --config ./.prettierrc --check \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "format:write": "prettier --config ./.prettierrc --write \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\" --list-different",
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "gen-dto": "ts-node --project ./swagger/tsconfig.json ./swagger/openapi-generator.ts",
    "build-schematics": "tsc -p ./schematics/tsconfig.schematics.json",
    "build-schematics:watch": "tsc -p ./schematics/tsconfig.schematics.json --watch",
    "run-schematics:theme-colors": "schematics ./schematics/collection.json:theme-colors theme-colors --out-path=./src/app/shared/constants --debug=false --force",
    "run-schematics:fonts": "schematics ./schematics/collection.json:fonts fonts --out-path=./src/app/shared/constants --debug=false --force",
    "source-map-explorer": "source-map-explorer ./dist/*.js",
    "openapi-cli": "openapi-generator-cli",
    "openapi-cli:generate": "openapi-generator-cli generate",
    "build": "ng build",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.7",
    "@angular-devkit/schematics": "^16.1.7",
    "@angular-devkit/schematics-cli": "^16.1.7",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.7",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.1.3"
  }
}
